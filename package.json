      "express-generator": "^4.14.1",
      "jest": "^29.5.0",
      "nodemon": "^2.0.22",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.9.1",
      "typescript": "^5.0.4"
   },
   "dependencies": {
      "@types/readable-stream": "^2.3.15",
